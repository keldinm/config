set ruler
set number
set hlsearch
set expandtab
set autoindent
set ignorecase
set tabstop=2
set shiftwidth=2
colorscheme peachpuff
syntax on
highlight Comment ctermfg=gray
highlight Search ctermbg=darkred
highlight Search ctermfg=white
inoremap jk <Esc>
set backspace=indent,eol,start
autocmd FileType make setlocal noexpandtab
